@{
    ViewBag.Title = "Tukar Poin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@ViewBag.Title</h2>

<div class="d-flex flex-wrap justify-content-center">
    @{ for(int i = 0; i < 10; i++)
        {
            <div class="card m-2" style="width: 250px;">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSlsFyJEXEkOe37FOcV-6wpR2jIvlWTWdBwNA&usqp=CAU" class="card-img-top" />
                <div class="card-body">
                    <h5 class="card-title">Kucing</h5>
                    <p class="card-text text-muted">100 poin</p>
                    <a class="btn btn-primary rounded-pill" style="width: 100%">Tukar</a>
                </div>
            </div>
        }
    }
</div>
